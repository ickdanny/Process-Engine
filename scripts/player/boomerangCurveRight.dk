setRotation(randomAngle());
let currentAngle = entityAngle();
let baseIncrement = 8.0;
let fallOff = 0.94;
let multiplier = 1.0;
while(multiplier > 0.15){
    currentAngle = currentAngle + (baseIncrement * multiplier);
    multiplier = multiplier * fallOff;
    setAngle(currentAngle);
    stall();
}