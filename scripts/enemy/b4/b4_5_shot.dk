let tick = 50;
let symmetry = diff(15, 20, 25, 30);
let initSpeed = 1.5;

let position;
func spawnBullet(velocity){
    spawn("sharpLRed", position, velocity, "b4_5_bullet");
}

while(true){
    position = entityPosition();
    ring(makePolar(initSpeed, randomAngle()), symmetry, spawnBullet);
    timer(tick);
}