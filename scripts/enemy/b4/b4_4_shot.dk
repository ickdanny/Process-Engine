let tick = diff(65, 65, 55, 55);
let symmetry = diff(3, 4, 5, 6);
let initSpeed = 2.9;
let angle = randomAngle();
let angleIncrement = symmetry * phi;

let position = entityPosition();
func spawnBullet(velocity){
    spawn("largeClear", position, velocity, "b4_4_bullet1");
}

while(true){
    ring(makePolar(initSpeed, angle), symmetry, spawnBullet);
    angle = angle + angleIncrement;
    timer(tick);
}
