let offsetRight = makeVector(45.0, -12.0);
let offsetLeft = flipY(offsetRight);
let tick = diff(75, 72, 69, 65);
let side = false;

func spawnTrap(position){
    spawn("trap", position, zeroPolar, "b2_3_shot1_trapLifetime");
}

while(true){
    if(side){
        spawnTrap(entityPosition() + offsetRight);
        side = false;
    }
    else{
        spawnTrap(entityPosition() + offsetLeft);
        side = true;
    }
    timer(tick);
}