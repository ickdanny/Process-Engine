let symmetryLow = diff(5, 6, 6, 7);
let angle = randomAngle();
let speedLow = 1.5;
let speedHigh = 2.0;

let position = entityPosition();
func spawnBullet(velocity){
    velocity = setR(velocity, random(speedLow, speedHigh));
    spawn("sharpClear", position, velocity, bulletScriptID);
}

let symmetry = symmetryLow;
if(chance(0.5)){
    symmetry = symmetry + 1;
}
ring(makePolar(0.0, angle), symmetry, spawnBullet);